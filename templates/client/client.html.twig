                                        <label class="checkbox inline no_indent">
                                            <input type="checkbox" id="inlineCheckbox6" value="option6"> 4 pièces
                                        </label>
                                        <label class="checkbox inline no_indent">
                                            <input type="checkbox" id="inlineCheckbox7" value="option7"> 5 pièces
                                        </label>
                                        <label class="checkbox inline no_indent">
                                            <input type="checkbox" id="inlineCheckbox8" value="option8"> 6 pieces
                                        </label>
                                        <label for="" class="col-sm-2 col-xs-12 control-label" id="hides2"></label>
